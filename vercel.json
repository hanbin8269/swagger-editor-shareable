{
    "name": "swagger-editor-shareable",
    "version": 2,
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "outputDirectory": ".next",
    "functions": {
      "src/app/**/*.{js,ts,jsx,tsx}": {
        "runtime": "nodejs18.x"
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }